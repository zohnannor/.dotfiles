    "git.discardUntrackedChangesToTrash": true,
    "git.autofetch": true,
    "git.blame.editorDecoration.enabled": false,
    "git.blame.statusBarItem.enabled": false,
    "scm.defaultViewMode": "tree",
    "git-graph.dialog.merge.noFastForward": false,
    "git-graph.dialog.merge.squashCommits": true,
    "diffEditor.maxComputationTime": 0,
    "diffEditor.experimental.showMoves": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": true,
    "diffEditor.experimental.useTrueInlineView": true,
    /* DISABLE AI */
    "chat.agent.enabled": false,
    "chat.agent.maxRequests": 0,
    "chat.checkpoints.enabled": false,
    "chat.commandCenter.enabled": false,
    "chat.detectParticipant.enabled": false,
    "chat.extensionTools.enabled": false,
    "chat.implicitContext.enabled": {
        "panel": "never"
    },
    "chat.implicitContext.suggestedContext": false,
    "chat.mcp.autostart": "never",
    "chat.mcp.discovery.enabled": {
        "claude-desktop": false,
        "windsurf": false,
        "cursor-global": false,
        "cursor-workspace": false
    },
    "chat.sendElementsToChat.enabled": false,
    "chat.tools.terminal.autoApprove": {
        ".*": false
    },
    "githubPullRequests.experimental.chat": false,
    "gitlab.duo.enabledWithoutGitlabProject": false,
    "gitlab.duoAgentPlatform.enabled": false,
    "gitlab.duoChat.enabled": false,
    "gitlab.duoCodeSuggestions.enabled": false,
    "gitlens.ai.enabled": false,
    "notebook.experimental.generate": false,
    "python.analysis.aiCodeActions": {
        "implementAbstractClasses": false,
        "generateSymbol": false,
        "convertFormatString": false
    },
    "supermaven.enableFixWithSupermaven": false,
    "telemetry.feedback.enabled": false,
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "gitlens.plusFeatures.enabled": false,
    "scm.showInputActionButton": false,
    "workbench.settings.showAISearchToggle": false,
    "chat.mcp.access": "none",
    "chat.disableAIFeatures": true,
    "githubPullRequests.codingAgent.codeLens": false,
    "chat.agent.codeBlockProgress": false,
    "githubPullRequests.codingAgent.autoCommitAndPush": false,
    "githubPullRequests.codingAgent.enabled": false,
    /* UNSORTED */
    "coverage-gutters.coverageBaseDir": "target",
    "coverage-gutters.customizable.context-menu": false,
    "coverage-gutters.showGutterCoverage": false,
    "coverage-gutters.showLineCoverage": true,
    "cSpell.enabled": false,
    "cSpell.language": "en,ru,en-US",
    "cSpell.minWordLength": 2,
    "css.lint.boxModel": "warning",
    "css.lint.compatibleVendorPrefixes": "warning",
    "css.lint.duplicateProperties": "warning",
    "css.lint.float": "warning",
    "css.lint.idSelector": "warning",
    "css.lint.ieHack": "warning",
    "css.lint.important": "warning",
    "css.lint.importStatement": "warning",
    "css.lint.universalSelector": "warning",
    "css.lint.unknownVendorSpecificProperties": "warning",
    "css.lint.zeroUnits": "warning",
    "scss.lint.boxModel": "warning",
    "scss.lint.compatibleVendorPrefixes": "warning",
    "scss.lint.duplicateProperties": "warning",
    "scss.lint.float": "warning",
    "scss.lint.idSelector": "warning",
    "scss.lint.ieHack": "warning",
    "scss.lint.important": "warning",
    "scss.lint.importStatement": "warning",
    "scss.lint.universalSelector": "warning",
    "scss.lint.unknownVendorSpecificProperties": "warning",
    "scss.lint.zeroUnits": "warning",
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "emmet.triggerExpansionOnTab": true,
    "emojisense.languages": {
        "markdown": true,
        "plaintext": {
            "markupCompletionsEnabled": false,
            "emojiDecoratorsEnabled": false
        },
        "scminput": true,
        "git-commit": true,
        "typescript": true,
        "javascript": true,
        "typescriptreact": true
    },
    "files.associations": {
        "*.html": "html"
    },
    "files.eol": "\n",
    "files.watcherExclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.git/**": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.metals": true,
        "**/node_modules/*/**": true,
        "**/target/*/**": true
